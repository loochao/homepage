' ST Magazine nø32.
' GfA Basic 2 & 3, basse r‚solution.
'
FILESELECT "A:\*.PI1","",nom$
IF EXIST(nom$)<>0
  ecran$=SPACE$(32000)
  BLOAD nom$,V:ecran$
  BMOVE V:ecran$+34,XBIOS(3),32000
  ~XBIOS(6,L:V:ecran$+2)
  COLOR 0
  LINE 0,199,319,199
  LINE 0,0,319,0
  REPEAT
    FOR i&=32000 TO f STEP -320
      a%=ADD(SUB(31840,i&),XBIOS(3))
      b%=ADD(XBIOS(3),i&)
      BMOVE b%,ADD(b%,320),160
      BMOVE a%,SUB(a%,320),160
    NEXT i&
    INC com|
    ADD f,320
  UNTIL com|=100
ENDIF
