' ST Magazine nø31.
' GfA Basic 3, haute r‚solution.
'
PCIRCLE 11,11,8
GET 0,0,22,22,b$
vx=1
REPEAT
  PUT x,y,b$
  x=x+vx
  y=y+vy
  vy=vy+0.01
  IF x<-3 OR x>621
    vx=-vx
  ENDIF
  IF y>380
    y=380
    vy=-vy+0.2
    IF ABS(vy)<0.1
      vx=vx-SGN(vx)*0.001
    ENDIF
  ENDIF
UNTIL ABS(vx)<1.0E-09
