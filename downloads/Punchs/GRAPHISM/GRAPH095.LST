RESERVE 40000
FILESELECT "A:\*.PI1","",nom$
ecran$=SPACE$(32000)
BLOAD nom$,V:ecran$
z%=GEMDOS(72,L:32000)
BMOVE V:ecran$+34,z%,32000
~XBIOS(6,L:V:ecran$+2)
CLS
ELLIPSE 159,99,159,99,900,2700
FOR i|=0 TO 198
  z%=ADD(z%,160)
  FOR x&=0 TO 159
    EXIT IF PTST(x&,i|)
  NEXT x&
  BMOVE z%,XBIOS(3)+31840,160
  FOR j=0 TO 319 STEP 320/(SUB(MUL(SUB(160,x&),2),1))
    PSET x&,i|,PTST(j,199)
    INC x&
  NEXT j
NEXT i|
